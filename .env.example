# Environment Variables for Docker Compose
# Copy this file to .env and fill in your actual values

# Github API
SHINYQ_GITHUB_TOKEN=your_github_token_here

# SHINYQ R2 Bucket
SHINYQ_R2_ACCOUNT_ID=your_cloudflare_account_id_here
SHINYQ_R2_ACCESS_KEY_ID=your_r2_access_key_id_here
SHINYQ_R2_SECRET_ACCESS_KEY=your_r2_secret_access_key_here
SHINYQ_R2_BUCKET_NAME=your_r2_bucket_name_here

# SHINYQ KV Cache
SHINYQ_R2_KV_API_TOKEN=your_kv_api_token_here
SHINYQ_R2_KV_NAMESPACE_ID=your_kv_namespace_id_here
